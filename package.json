{
  "name": "PulmonxOUS",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "npx react-native run-ios -- --reset-cache  --verbose",
    "start": "watchman watch-del-all && react-native start --reset-cache  --verbose",
    "test": "jest",
    "clean": "react-native-clean-project clean-project-auto",
    "compile": "tsc --noEmit -p . --pretty",
    "format": "npm-run-all format:*",
    "format:js": "prettier --write {.,**}/*.js",
    "format:json": "prettier --write {.,**}/*.json",
    "format:md": "prettier --write {.,**}/*.md",
    "format:ts": "prettier --write {.,**}/*.{ts,tsx}",
    "hack:types-react-navigation": "rimraf node_modules/@types/react-navigation/node_modules/@types",
    "hack:types-react-native": "rimraf node_modules/@types/react-native/node_modules/@types",
    "hack:types-react-test-renderer": "rimraf node_modules/@types/react-test-renderer/node_modules/@types",
    "lint": "eslint index.js app --fix --ext .js,.ts,.tsx",
    "patch": "patch-package",
    "postinstall": "node ./bin/postInstall",
    "prepare": "npm-run-all patch hack:*",
    "storybook": "start-storybook -p 9001 -c ./storybook",
    "sdk": "graphql-codegen --config codegen.yml",
    "sdk:watch": "nodemon --watch 'app/sdk/queries/**/**' --ext '*' -x 'yarn sdk'",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@beeman/react-native-safe-area-view": "^1.0.0",
    "@eva-design/eva": "^2.0.0",
    "@expo/react-native-action-sheet": "^3.7.0",
    "@graphql-codegen/cli": "^1.13.5",
    "@graphql-codegen/introspection": "^1.13.5",
    "@graphql-codegen/typescript": "^1.13.5",
    "@graphql-codegen/typescript-operations": "^1.13.5",
    "@graphql-codegen/typescript-react-apollo": "^1.13.5",
    "@react-native-community/async-storage": "^1.10.3",
    "@react-native-community/datetimepicker": "^3.0.2",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/netinfo": "^5.9.6",
    "@react-navigation/native": "^5.4.0",
    "@types/lodash": "^4.14.160",
    "apisauce": "1.1.1",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-context": "^1.0.20",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "buffer": "^5.6.0",
    "date-fns": "^2.14.0",
    "formik": "^2.1.4",
    "graphql": "15.0.0",
    "i18n-js": "^3.5.1",
    "inflection": "^1.12.0",
    "infusionsoft-javascript-api": "^0.3.1",
    "lodash": "^4.17.15",
    "lodash.throttle": "4.1.1",
    "mobx": "^5.15.4",
    "mobx-react-lite": "^2.0.7",
    "mobx-state-tree": "^3.16.0",
    "moment-timezone": "^0.5.31",
    "native-base": "^2.13.12",
    "node-libs-react-native": "^1.2.0",
    "q": "^1.5.1",
    "ramda": "0.27.0",
    "react": "16.13.1",
    "react-native": "0.63.2",
    "react-native-calendars": "^1.300.0",
    "react-native-datepicker": "^1.7.2",
    "react-native-elements": "^2.0.0",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-inappbrowser-reborn": "^3.4.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-keychain": "4.0.0",
    "react-native-loading-spinner-overlay": "^1.1.0",
    "react-native-localize": "^1.4.0",
    "react-native-modal-datetime-picker": "^9.0.0",
    "react-native-multi-selectbox": "^1.3.0",
    "react-native-multiple-select": "^0.5.5",
    "react-native-multiple-select-list": "^1.0.4",
    "react-native-reanimated": "^1.8.0",
    "react-native-safe-area-context": "1.0.2",
    "react-native-safe-area-view": "1.1.1",
    "react-native-screens": "^2.7.0",
    "react-native-select-multiple": "^2.1.0",
    "react-native-simple-toast": "^1.1.2",
    "react-native-spinkit": "^1.5.0",
    "react-native-splash-screen": "3.2.0",
    "react-native-svg": "^12.1.0",
    "react-native-vector-icons": "^7.0.0",
    "react-navigation": "4.3.9",
    "react-navigation-stack": "2.5.1",
    "react-navigation-tabs": "^2.8.13",
    "reactotron-core-client": "^2.8.10",
    "reactotron-mst": "^3.1.3",
    "reactotron-react-native": "^5.0.0",
    "rn-lightweight-date-picker": "^1.1.0",
    "tslib": "^2.0.0",
    "typedef": "^1.1.0",
    "underscore": "^1.10.2",
    "validate.js": "0.13.1",
    "yup": "^0.28.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-optional-catch-binding": "^7.8.3",
    "@babel/runtime": "^7.9.6",
    "@storybook/addon-storyshots": "5.3.18",
    "@storybook/react-native": "5.3.18",
    "@storybook/react-native-server": "5.3.18",
    "@types/jest": "25.2.3",
    "@types/ramda": "0.27.5",
    "@types/react": "16.9.35",
    "@types/react-native": "0.63.2",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-jest": "^26.0.1",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-native": "^3.8.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "ignite-bowser": "^5.0.3",
    "jest": "^26.0.1",
    "jetifier": "^1.6.5",
    "lint-staged": "^10.2.4",
    "metro-react-native-babel-preset": "^0.59.0",
    "npm-run-all": "4.1.5",
    "patch-package": "6.2.2",
    "postinstall-prepare": "1.0.1",
    "prettier": "2.0.5",
    "react-devtools-core": "4.7.0",
    "react-native-clean-project": "^3.3.0",
    "react-native-dotenv": "^0.2.0",
    "react-powerplug": "1.0.0",
    "react-test-renderer": "16.13.1",
    "rimraf": "3.0.2",
    "solidarity": "3.0.0",
    "typescript": "3.9.2"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/node_modules/react-native/jest/setup.js",
      "<rootDir>/test/setup.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-native|react-navigation|@react-navigation|@storybook|@react-native-community)"
    ]
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "prettier",
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:react-native/all",
      "standard"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "project": "./tsconfig.json"
    },
    "plugins": [
      "@typescript-eslint",
      "react",
      "react-native"
    ],
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    },
    "globals": {
      "__DEV__": false,
      "jasmine": false,
      "beforeAll": false,
      "afterAll": false,
      "beforeEach": false,
      "afterEach": false,
      "test": false,
      "expect": false,
      "describe": false,
      "jest": false,
      "it": false
    },
    "rules": {
      "comma-dangle": 0,
      "no-unused-vars": 0,
      "no-undef": 0,
      "quotes": 0,
      "spaced-comment": 0,
      "react/no-unescaped-entities": 0,
      "react/prop-types": "off",
      "react-native/no-raw-text": 0,
      "react-native/no-inline-styles": 0,
      "react-native/no-color-literals": 0,
      "space-before-function-paren": 0,
      "@typescript-eslint/ban-ts-ignore": 0,
      "@typescript-eslint/camelcase": 0,
      "@typescript-eslint/explicit-member-accessibility": 0,
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/indent": 0,
      "@typescript-eslint/member-delimiter-style": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/no-object-literal-type-assertion": 0,
      "@typescript-eslint/no-empty-interface": 0,
      "@typescript-eslint/no-var-requires": 0
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,less,md,ts,tsx,html,component.html,graphql}": [
      "prettier --write"
    ]
  }
}
